probes
{
// Where to load it from
functionObjectLibs ( "libsampling.so" );

type        probes;

// Name of the directory for probe data
name        probes;

// Fields to be probed
fields (
        CO2
        O2
	CH4
	H2O
       );

probeLocations
(
   ( 1.6 1.6 1.6 )
   ( 1.4 1.4 1.4 )

);
}
